{
  "name": "wddoc",
  "description": "wddoc is a documentation generator based on Dox and Markdown with support for JavaScript, CoffeeScript and IcedCoffeeScript.",
  "version": "0.1.0",
  "author": "Charles Bourasseau <charles.bourasseau@gmail.com>",
  "contributors": [],
  "dependencies": {
    "dox": "~0.4.4",
    "commander": "~2.2.0",
    "async": "~0.8.0",
    "ejs": "~1.0.0",
    "deepmerge": "~0.2.7"
  },
  "devDependencies": {
    "mocha": ">= 1.3.2",
    "should": ">= 1.1.0"
  },
  "bin": {
    "wddoc": "./bin/wddoc"
  },
  "keywords": [
    "documentation",
    "dox",
    "markdown",
    "webdriver",
    "webdriverjs"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:webdriverjs/wddoc.git"
  },
  "main": "index",
  "license": "MIT",
  "scripts": {
    "documentation": "bin/wddoc -o doc/api.md lib/wddoc.js",
    "test": "./node_modules/mocha/bin/mocha test/*-test.js",
    "examples": "node examples/example.js"
  },
  "engines": {
    "node": ">= 0.6.0"
  }
}